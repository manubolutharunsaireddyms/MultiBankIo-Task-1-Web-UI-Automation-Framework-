<?xml version="1.0" encoding="UTF-8"?>
<suite name="Suite" parallel="tests" thread-count="5">

	<listeners>
		<listener class-name="reports.ExtentTestListener" />
	</listeners>

	<test name="NavigationTests">
		<parameter name="browser" value="chrome" />
		<classes>
			<class name="tests.NavigationTests" />
		</classes>
	</test>
	<test name="TradingTests">
		<parameter name="browser" value="edge" />
		<classes>
			<class name="tests.TradingTests" />
		</classes>
	</test>
	<test name="ContentTests">
		<parameter name="browser" value="chrome" />
		<classes>
			<class name="tests.ContentTest" />
		</classes>
	</test>
</suite>
